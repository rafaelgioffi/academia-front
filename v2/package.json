{
  "name": "academia-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^5.10.10",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.12.2",
    "formik": "^2.4.6",
    "prop-types": "^15.8.1",
    "qs": "^6.14.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1",
    "react-scripts": "^5.0.1",
    "styled-components": "^6.1.19",
    "web-vitals": "^2.1.4",
    "yup": "^1.7.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "CI=true react-scripts test --coverage --watchAll=false --logHeapUsage --silent --maxWorkers=4"
  },
  "jest": {
    "resetMocks": false,
    "coverageReporters": [
      "lcov"
    ],
    "transformIgnorePatterns": [
      "node_modules\/(?!axios)"],
      "coverageThreshold": {
        "global": {
          "branches": 90,
          "functions": 90,
          "lines": 90,
          "statements": 90
        }      
  },
  "collectCoverageFrom": [
    "src/styles/theme.js",
    "src/services/projects/*",
    "src/components/PageTitle/*",
    "src/components/Button/*"
  ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^19.1.1",
    "@testing-library/react-hooks": "^5.1.3",
    "@testing-library/user-event": "^13.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
